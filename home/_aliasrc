#zshmarks
alias g="jump"
alias s="bookmark"
alias d="deletemark"
alias l="showmarks"

#generic
alias cal="gcal"
alias tunnel="ssh  -q -N -f -D 9999 vanyli.net"
alias ppjson="python -m json.tool"
alias flushdns="sudo dscacheutil -flushcache;sudo killall -HUP mDNSResponder"

#git
alias gpl="git pull"
alias gps="git push"
alias rmerged="git branch --merged | egrep -v \"(^\*|master|dev)\" | xargs git branch -d"

alias dynamo="dynamodb-local -inMemory -sharedDb" 

function randmac {
    sudo ifconfig en0 ether $1
}

function take {
    mkdir -p $1 && cd $1
}

function fag {
    if [ -z ${1} ]; then
	echo "find . -type -f -exec grep -l '' {} \;"
	return 0
    fi;

    echo "looking for: ${1}"
    find . -type f -exec grep -l $1 {} \;
}

function server {
    python3 -m http.server 8090
}

